# AWS Credentials and Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=your_aws_region_here # e.g., us-east-1

# S3 Bucket Names
S3_IMAGE_BUCKET=your_s3_original_images_bucket_name_here
S3_THUMBNAIL_BUCKET=your_s3_thumbnails_bucket_name_here

# Database Configuration (MySQL/RDS)
DB_HOST=your_rds_endpoint_here # e.g., comp5349a2-rdsinstance.xxxxxx.us-east-1.rds.amazonaws.com
DB_PORT=3306
DB_NAME=ImageAnnotationDB
DB_USER=dbadmin
DB_PASSWORD=your_db_password_here

# Flask Application Configuration
FLASK_APP=web_app.app
FLASK_ENV=development # or production
FLASK_DEBUG=True # Set to False in production
FLASK_SECRET_KEY=your_flask_secret_key_here # A strong, random string
LOG_LEVEL=INFO # DEBUG, INFO, WARNING, ERROR

# Lambda Function Names (if invoked directly or for configuration)
ANNOTATION_LAMBDA_NAME=your_annotation_lambda_function_name
THUMBNAIL_LAMBDA_NAME=your_thumbnail_lambda_function_name 